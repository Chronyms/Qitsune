{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "appstract/laravel-opcache": "^2.0",
        "barryvdh/laravel-debugbar": "^3.0",
        "codedge/laravel-fpdf": "^1.2",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~4.0",
        "intervention/image": "^2.4",
        "jeremykenedy/laravel-https": "^1.1",
        "jeremykenedy/laravel-logger": "^2.2",
        "jeremykenedy/laravel-roles": "^1.1",
        "jeremykenedy/uuid": "^3.1",
        "kouz/laravel-airbrake": "^0.4.1",
        "laravel/framework": "5.7.*",
        "laravelcollective/html": "^5.5",
        "rap2hpoutre/laravel-log-viewer": "^1.0.0",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "spatie/laravel-cookie-consent": "^2.2"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~2.0",
        "phpunit/phpunit": "~8.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
